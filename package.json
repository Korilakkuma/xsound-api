{
  "name": "xsound-api",
  "version": "2.0.0",
  "description": "XSound API documentation",
  "main": "./assets/app.js",
  "scripts": {
    "clean": "rm -rf ./assets/app.* ./assets/vendor.*",
    "lint:js": "eslint src",
    "lint:css": "stylelint src",
    "lint": "run-p lint:js lint:css",
    "fix:js": "eslint --fix src",
    "fix:css": "stylelint --fix src",
    "fix": "run-p lint:js lint:css",
    "build": "npm run clean && webpack --progress --mode production",
    "dev": "webpack-cli serve --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "eslint --fix"
    ],
    "./src/**/*.css": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/xsound-api.git"
  },
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/xsound-api/issues"
  },
  "homepage": "https://xsound.dev/",
  "dependencies": {
    "connected-react-router": "^6.9.1",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-react": "^7.13.13",
    "@webpack-cli/serve": "^1.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.23.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^5.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
