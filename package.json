{
  "name": "xsound-api",
  "version": "2.0.0",
  "description": "XSound API documentation",
  "main": "./app.js",
  "scripts": {
    "lint:js": "eslint ./src/*.js ./src/**/*.js",
    "lint:css": "stylelint ./src/*.css ./src/**/*.css",
    "lint": "run-p lint:js lint:css",
    "build": "webpack --mode production",
    "build:watch": "webpack --watch --mode development",
    "docker:build": "docker build -t app .",
    "docker:rmi": "docker rmi app",
    "start": "docker container run --name webserver -d -p 3000:80 -v $(pwd):/usr/share/nginx/html app",
    "stop": "docker stop webserver && docker rm webserver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/xsound-api.git"
  },
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/xsound-api/issues"
  },
  "homepage": "https://korilakkuma.github.io/xsound-api/",
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mini-css-extract-plugin": "^0.4.2",
    "npm-run-all": "^4.1.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  }
}
